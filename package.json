{
  "name": "express-db-manager",
  "version": "0.0.1",
  "description": "express application to manage databases",
  "main": "index.js",
  "preferGlobal": true,
  "bin": {
    "dbmanager": "./bin/dbmanager"
  },
  "scripts": {
    "postinstall": "./node_modules/.bin/bower install",
    "start": "DEBUG=express:db-manager* ./bin/dbmanager",
    "test": "./node_modules/.bin/grunt test",
    "deps": "npm list --depth=0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Naxmeify/express-db-manager.git"
  },
  "keywords": [
    "express",
    "application",
    "databases",
    "manager"
  ],
  "author": "MRW Neundorf <matt@nax.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Naxmeify/express-db-manager/issues"
  },
  "homepage": "https://github.com/Naxmeify/express-db-manager",
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-cli": "^0.1.13",
    "grunt-mocha-cli": "^1.14.0",
    "mocha": "^2.2.5",
    "should": "^7.0.4",
    "supertest": "^1.1.0"
  },
  "dependencies": {
    "body-parser": "^1.13.3",
    "bower": "^1.5.2",
    "coffee-script": "^1.9.3",
    "connect-mincer": "^1.0.0",
    "cookie-parser": "^1.3.5",
    "debug": "^2.2.0",
    "express": "^4.13.3",
    "express-routes-manager": "0.0.2",
    "express-session": "^1.11.3",
    "flash": "^1.1.0",
    "jade": "^1.11.0",
    "less": "^2.5.1",
    "method-override": "^2.3.5",
    "mincer": "^1.3.0",
    "minimist": "^1.2.0",
    "morgan": "^1.6.1",
    "serve-favicon": "^2.3.0",
    "stylus": "^0.52.0"
  }
